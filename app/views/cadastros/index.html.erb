            <td><%= cadastro.end %></td>
            <td><%= cadastro.complemento %></td>
            <td><strong><%= link_to 'Mostrar', cadastro %></strong></td>
            <td><strong><%= link_to 'Editar', edit_cadastro_path(cadastro) %></strong></td>
            <td><strong><%= link_to 'Deletar', cadastro, method: :delete, data: { confirm: 'Deseja excluir o cadastro?' } %></strong> </td>
