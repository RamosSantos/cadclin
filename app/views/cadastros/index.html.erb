            </tr>

      <tbody>
        <% @cadastros.each do |cadastro| %>
          <tr>
            <td><%= cadastro.nome_aluno %></td>
            <td><%= cadastro.rm %></td>
            <td><%= cadastro.ra %></td>
            <td><%= cadastro.nome_pai %></td>
